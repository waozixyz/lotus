@app-pict
	:&name
	:&on-draw
	:&on-mouse
	:&on-button

&on-button ( win* -> ) POP2 BRK
&on-mouse ( win* -> ) POP2 BRK

&on-draw ( win* -- )

	( set pos )
	LDA2k #0008 ADD2 .Screen/x DEO2
	INC2 INC2 LDA2 #0010 ADD2 .Screen/y DEO2

	;&target DUP2 ;scap JSR2 #0009 SUB2 ;read-size JSR2 ;draw-image JSR2

JMP2r

	&target "etc/akane20x10.icn $1

&name "<pict> $1
