@app-potato
	:potato-manifest
	:&on-draw
	:&on-mouse
	:&on-button

&on-button ( win* -> ) POP2 BRK
&on-mouse ( win* -> ) POP2 BRK

&on-draw ( win* -- )

	LDA2k #0008 ADD2 .Screen/x DEO2
	INC2 INC2 LDA2 #0010 ADD2 .Screen/y DEO2
    ;&prompt ;draw-str JSR2 POP2

JMP2r
	&path ". $1
	&prompt "~> 20 "LIST $1

@potato-manifest

	01 "Potato $1
		01 'n :close-win "Close $1
	$1
