( devices )
|00 @System/vector $2 &expansion $2 &wst $1 &rst $1 &metadata $2 &r $2 &g $2 &b $2 &debug $1 &state $1
|10 @Console/vector $2 &read $5 &type $1 &write $1 &error $1
|20 @Screen/vector $2 &width $2 &height $2 &auto $1 &pad $1 &x $2 &y $2 &addr $2 &pixel $1 &sprite $1
|80 @Controller/vector $2 &button $1 &key $1
|90 @Mouse/vector $2 &x $2 &y $2 &state $1 &pad $3 &sx $2 &sy $1 &sy-lb $1
|a0 @File/vector $2 &success $1 &success-lb $1 &stat $2 &delete $1 &append $1 &name $2 &length $2 &read $2 &write $2

( variables )
|0000
@circle-cx      $2
@circle-cy      $2
@circle-r       $2
@circle-rsq     $2
@r-direction    $1
@timer          $1
@anim-delay     $1
@anim-delay-inhale $1
@current-val $1
@max-breath     $1
@state          $1
@view         $1

( constants )
%CIRCLE-R-MIN  { #0020 }
%CIRCLE-R-MAX  { #0035 }

( --- Tabbar Layout --- )
%TABBAR-HEIGHT { #0010 }
%TABBAR-Y      { #0000 }

( --- Sidebar Anchor Layout --- )
%SIDEBAR-X      { #0008 }
%SIDEBAR-Y      { #0000 }
%BTN-SIZE       { #0008 }

( --- Bitmasks --- )
%MSK-PLAYING  { #01 }
%MSK-OPEN     { #02 }
%MSK-PRESSED  { #04 }

%SCR-TITLE    { #01 }
%SCR-BREATH   { #02 }
%SCR-HOLD     { #03 }
%SCR-INHALE   { #04 }
%SCR-RESULT   { #05 }

( main program )
|0100
    ;meta #06 DEO2

    #012e .Screen/width DEO2
    #00f0 .Screen/height DEO2


    load-theme

    CIRCLE-R-MIN .circle-r STZ2
    #03 .anim-delay STZ
    #04 .anim-delay-inhale STZ
    #00 .current-val STZ
    #1e .max-breath STZ

    MSK-OPEN .state STZ
    SCR-BREATH .view STZ

    ;on-frame .Screen/vector DEO2
    
    ,&skip-this JCN
    inbe-rom-path ;load-rom JSR2
    &skip-this
BRK

@meta $1
	( name ) "Inner "Breeze 0a
	( details ) "A 20 "guided 20 "breathing 20 "meditation 0a
	( author ) "By 20 "Waozi 0a
	( date ) &date "18 20 "Feb 20 "2026 $1
	01
	( icon ) 83 =appicon

@load-theme ( -- )
	;&path .File/name DEO2
	#0002 .File/length DEO2
	;&r .File/read DEO2
	;&g .File/read DEO2
	;&b .File/read DEO2
	.File/success DEI2 ORA #01 [ JCN JMP2r ]
	LIT2 &r $2 .System/r DEO2
	LIT2 &g $2 .System/g DEO2
	LIT2 &b $2 .System/b DEO2

JMP2r
	&path ".theme $1

@breath-logic
    ( --- Normal Breathing Logic --- )
    .timer LDZ .anim-delay LDZ LTH ,&return JCN
    #00 .timer STZ

    .r-direction LDZ #01 EQU ,&shrink JCN

    &grow
        .circle-r LDZ2 INC2 .circle-r STZ2
        .circle-r LDZ2 CIRCLE-R-MAX LTH2 ,&return JCN
        #01 .r-direction STZ
        ,&return JMP

    &shrink
        .circle-r LDZ2 #0001 SUB2 .circle-r STZ2
        .circle-r LDZ2 CIRCLE-R-MIN GTH2 ,&return JCN
        #00 .r-direction STZ

        ( Breath Counter Logic )
        .current-val LDZ INC .current-val STZ
        .current-val LDZ .max-breath LDZ LTH ,&return JCN
        
        SCR-HOLD .view STZ
        #00 .current-val STZ

    &return
    JMP2r
 
( --- Hold Logic: Increment every 60 frames --- )
@hold-logic
    ( Increment once per second )
    .timer LDZ #3c LTH ,&draw JCN
    #00 .timer STZ
    .current-val LDZ INC .current-val STZ

    &draw
    ( Set UI positions )
    .circle-cx LDZ2 #0014 SUB2 .Screen/x DEO2
    .circle-cy LDZ2 #0035 ADD2 .Screen/y DEO2
    ;fill-icn .Screen/addr DEO2

    &loop-bg
    #03 .Screen/sprite DEO
    .Screen/x DEI2 INC2 DUP2 .Screen/x DEO2
    .circle-cx LDZ2 #0015 ADD2 LTH2 ,&loop-bg JCN

    ( Draw Label )
    .circle-cx LDZ2 #0014 SUB2 .Screen/x DEO2
    .circle-cy LDZ2 #0035 ADD2 .Screen/y DEO2
    ;txt-breath ;draw-zstr JSR2

    JMP2r

@inhale-logic
    ( State Machine: 0=Inhale, 1=Hold, 2=Exhale )
    .r-direction LDZ #01 EQU ,&hold JCN
    .r-direction LDZ #02 EQU ,&exhale JCN

    &inhale
        .timer LDZ .anim-delay-inhale LDZ LTH ,&check JCN
        #00 .timer STZ
        .circle-r LDZ2 INC2 .circle-r STZ2
        .circle-r LDZ2 CIRCLE-R-MAX LTH2 ,&return JCN
        ( Hit Max: Switch to Hold State )
        #01 .r-direction STZ
        #00 .current-val STZ ( Reset frame counter )
        #00 .timer STZ       ( Reset seconds counter )
        ,&return JMP

    &exhale
        .timer LDZ .anim-delay-inhale LDZ LTH ,&check JCN
        #00 .timer STZ
        .circle-r LDZ2 #0001 SUB2 .circle-r STZ2
        .circle-r LDZ2 CIRCLE-R-MIN GTH2 ,&return JCN
        ( Hit Min: Reset and go back to Breath screen )
        #00 .r-direction STZ
        #00 .current-val STZ
        SCR-BREATH .view STZ
        ,&return JMP

    &check
    ( If not in hold state, just return )
    .r-direction LDZ #01 NEQ ,&return JCN

    &hold
        ( We use .current-val to count 60 frames )
        .timer LDZ #3c LTH ,&return JCN
        #00 .timer STZ
        .current-val LDZ INC .current-val STZ
        
        ( Check if we reached 15 seconds )
        .current-val LDZ #0f LTH ,&return JCN
        
        ( 15 Seconds Done: Switch to Exhale State )
        #02 .r-direction STZ
        #00 .timer STZ
        
    &return
    JMP2r

@on-frame
    #00 .Screen/auto DEO
    ;handle-mouse JSR2
    ;clear-screen JSR2
    ;draw-ui JSR2


    .Screen/width DEI2 #01 SFT2 #0020 SUB2 .Screen/x DEO2
    #0000 .Screen/y DEO2

    SCR-BREATH .view LDZ NEQ ,&skip-breath-text JCN
    ;txt-breath ;draw-zstr JSR2

    &skip-breath-text

    SCR-HOLD .view LDZ NEQ ,&skip-hold-text JCN
    ;txt-hold ;draw-zstr JSR2
    ;txt-it ;draw-zstr JSR2

    &skip-hold-text

    SCR-INHALE .view LDZ NEQ ,&skip-inhale-text JCN
    ;txt-breath ;draw-zstr JSR2
    ;txt-in ;draw-zstr JSR2

    &skip-inhale-text


    ( Only process timing if MSK-PLAYING is set )
    .state LDZ MSK-PLAYING AND #00 EQU ,draw JCN

    (animate )
    .timer LDZ INC .timer STZ

    ( Check if we are in HOLD view )
    .view LDZ SCR-BREATH NEQ ,&no-breath JCN
        ;breath-logic JSR2
    &no-breath
    .view LDZ SCR-HOLD NEQ ,&no-hold JCN
        ;hold-logic JSR2
    &no-hold
    .view LDZ SCR-INHALE NEQ ,&no-inhale JCN
        ;inhale-logic JSR2
    &no-inhale

    @draw
    .Screen/width DEI2 #01 SFT2 .Screen/height DEI2 #01 SFT2 .circle-r LDZ2
    ;draw-circle JSR2
    ;draw-center-stats JSR2

    .Mouse/x DEI2 .Screen/x DEO2 .Mouse/y DEI2 .Screen/y DEO2
    ;sprite-cursor .Screen/addr DEO2 #01 .Screen/sprite DEO
BRK2

@handle-back-button ( -- )
	.Mouse/y DEI2 #0010 LTH2
    .Mouse/x DEI2 #0100 GTH2 AND ,click-back JCN
    JMP2r
	@click-back
    ;potato-rom-path ;load-rom JSR2

@handle-hold-button ( -- )
    .view LDZ SCR-HOLD NEQ ,&skip JCN

    ( Check Y: circle-cy + 0x35 <= Mouse/y < circle-cy + 0x3d )
    .circle-cy LDZ2 #0035 ADD2
    DUP2 .Mouse/y DEI2 SWP2 LTH2 ,&skip JCN
    #0008 ADD2
    .Mouse/y DEI2 SWP2 GTH2 ,&skip JCN

    ( Check X: circle-cx - 0x14 <= Mouse/x < circle-cx + 0x1d )
    .circle-cx LDZ2 #0014 SUB2
    DUP2 .Mouse/x DEI2 SWP2 LTH2 ,&skip JCN
    #0031 ADD2
    .Mouse/x DEI2 SWP2 GTH2 ,&skip JCN

    ;to-inhale JMP2

    &skip JMP2r

@handle-mouse
    .Mouse/state DEI #00 EQU ;released JCN2
    .state LDZ MSK-PRESSED AND ;done JCN2
    .state LDZ MSK-PRESSED ORA .state STZ

    ;handle-back-button JSR2
    ;handle-hold-button JSR2

    ( Sidebar Toggle Check )
    .Mouse/y DEI2 SIDEBAR-Y LTH2 ,check-controls JCN
    .Mouse/y DEI2 SIDEBAR-Y BTN-SIZE ADD2 GTH2 ,check-controls JCN
    .Mouse/x DEI2 SIDEBAR-X #0048 ADD2 LTH2 ,check-controls JCN
    .Mouse/x DEI2 SIDEBAR-X #0048 ADD2 BTN-SIZE ADD2 GTH2 ,check-controls JCN
        .state LDZ MSK-OPEN EOR .state STZ
        ;done JMP2

    @check-controls
    ;handle-mouse-controls JSR2

    @done JMP2r
    @released
        .state LDZ MSK-PRESSED #ff EOR AND .state STZ
JMP2r

@handle-mouse-controls
    .state LDZ MSK-OPEN AND #00 EQU ,skip-controls JCN
    ;handle-speed-logic JSR2
    ;handle-breath-logic JSR2
    ;handle-play-logic JSR2
    @skip-controls JMP2r


@to-breath
    SCR-BREATH .view STZ
    #00 .current-val STZ
    CIRCLE-R-MIN .circle-r STZ2
    JMP2r

@to-hold
    SCR-HOLD .view STZ
    #00 .current-val STZ
    CIRCLE-R-MIN .circle-r STZ2
    JMP2r

@to-inhale
    .r-direction STZ #01
    SCR-INHALE .view STZ
    #00 .current-val STZ
    CIRCLE-R-MIN .circle-r STZ2
    JMP2r

@handle-play-logic
    #0094 row-hit? #00 EQU ;skip-play JCN2

    ( Seek Back - Offset #0000 )
    #0000 btn-hit? #00 EQU ,&check-play JCN

        ( If view == BREATH, just reset current-val )
        .view LDZ SCR-BREATH EQU ;to-breath JMP2

        ( If view == HOLD, go to BREATH )
        .view LDZ SCR-HOLD EQU ;to-breath JMP2

        ( If view == INHALE, go to HOLD )
        .view LDZ SCR-INHALE EQU ;to-hold JMP2

        ;skip-play JCN2 ( Safety skip )


    &check-play ( Play/Pause - Offset #0010 )
    #0010 btn-hit? #00 EQU ,&check-seek-forward JCN
        .state LDZ MSK-PLAYING EOR .state STZ
        ;skip-play JCN2

    &check-seek-forward ( Seek Forward - Offset #0020 )
    #0020 btn-hit? #00 EQU ;skip-play JCN2

        ( If view == BREATH, go to hold state )
        .view LDZ SCR-BREATH EQU ;to-hold JMP2

        ( If view == HOLD, go to inhale state )
        .view LDZ SCR-HOLD EQU ;to-inhale JMP2


        ;skip-play JCN2

    @skip-play JMP2r

@handle-speed-logic
    #0024 row-hit? #00 EQU ,skip-speed JCN

    #0000 btn-hit? #00 NEQ ,dec-speed JCN
    #0020 btn-hit? #00 EQU ,skip-speed JCN

    .anim-delay LDZ #1e EQU ,skip-speed JCN
    .anim-delay LDZ INC .anim-delay STZ
    ,skip-speed JMP

    @dec-speed
    .anim-delay LDZ #01 EQU ,skip-speed JCN
    .anim-delay LDZ #01 SUB .anim-delay STZ   ( DEC -> #01 SUB )

    @skip-speed JMP2r

@handle-breath-logic
    #004c row-hit? #00 EQU ,skip-breath JCN

    #0000 btn-hit? #00 NEQ ,dec-breath JCN
    #0020 btn-hit? #00 EQU ,skip-breath JCN

    .max-breath LDZ #2b EQU ,skip-breath JCN
    .max-breath LDZ INC .max-breath STZ
    ,skip-breath JMP

    @dec-breath
    .max-breath LDZ #01 EQU ,skip-breath JCN
    .max-breath LDZ #01 SUB .max-breath STZ   ( DEC -> #01 SUB )

    @skip-breath JMP2r

@draw-ui
    ;draw-tabbar JSR2
    ;draw-ui-controls JSR2
JMP2r

@draw-ui-controls
    ( Header Content )
    SIDEBAR-X .Screen/x DEO2 SIDEBAR-Y .Screen/y DEO2
    ;txt-controls ;draw-zstr JSR2
    SIDEBAR-X #0048 ADD2 .Screen/x DEO2 SIDEBAR-Y .Screen/y DEO2
    .state LDZ MSK-OPEN AND ,show-up JCN
        ;sprite-down .Screen/addr DEO2 ,render-toggle JMP
    @show-up ;sprite-up .Screen/addr DEO2
    @render-toggle #01 .Screen/sprite DEO

    ( Draw content if open )

    .state LDZ MSK-OPEN AND #00 EQU ;skip-children JCN2
        ( Speed Section )
        SIDEBAR-X .Screen/x DEO2 SIDEBAR-Y #0018 ADD2 .Screen/y DEO2
        ;txt-speed ;draw-zstr JSR2
        .anim-delay LDZ SIDEBAR-Y #0024 ADD2 ;draw-counter JSR2

        ( Breaths Section )
        SIDEBAR-X .Screen/x DEO2 SIDEBAR-Y #003c ADD2 .Screen/y DEO2
        ;txt-breaths ;draw-zstr JSR2
        .max-breath LDZ SIDEBAR-Y #004c ADD2 ;draw-counter JSR2

        ( Player Control Section )
        SIDEBAR-X .Screen/x DEO2 SIDEBAR-Y #0084 ADD2 .Screen/y DEO2
        ;txt-play ;draw-zstr JSR2
        SIDEBAR-Y #0094 ADD2 .Screen/y DEO2


        SIDEBAR-X .Screen/x DEO2
        ;sprite-seek .Screen/addr DEO2
        #11 .Screen/sprite DEO

        SIDEBAR-X #0010 ADD2 .Screen/x DEO2
        .state LDZ MSK-PLAYING AND ,show-pause JCN
        ;sprite-play .Screen/addr DEO2 ,render-player JMP
        @show-pause ;sprite-pause .Screen/addr DEO2
        @render-player
        #01 .Screen/sprite DEO

        SIDEBAR-X #0020 ADD2 .Screen/x DEO2
        ;sprite-seek .Screen/addr DEO2
        #01 .Screen/sprite DEO
    @skip-children
JMP2r

