	JMP2r

@draw-counter ( val y* -- )
    .Screen/y DEO2            ( Set Y coordinate )
    SIDEBAR-X .Screen/x DEO2  ( Start at sidebar X )

    ( Minus Sprite )
    ;sprite-minus .Screen/addr DEO2 #01 .Screen/sprite DEO

    ( Number )
    SIDEBAR-X #000c ADD2 .Screen/x DEO2
    ;draw-num JSR2            ( val is already on stack )

    ( Plus Sprite )
    SIDEBAR-X #0020 ADD2 .Screen/x DEO2
    ;sprite-plus .Screen/addr DEO2 #01 .Screen/sprite DEO
JMP2r


@draw-num ( val -- )
    DUP #0a DIV             ( Get Tens )
    DUP #30 ADD ;draw-char JSR2 ( Print Tens )
    #0a MUL SUB             ( Original - Tens*10 = Ones )
    #30 ADD ;draw-char JSR2 ( Print Ones )
JMP2r

@draw-char ( char -- )
    #20 SUB            ( Align: Space is now index 0 )
    #00 SWP            ( Byte to Short )
    #30 SFT2           ( Multiply by 8 )
    ;font ADD2 .Screen/addr DEO2
    #41 .Screen/sprite DEO
    .Screen/x DEI2 #0008 ADD2 .Screen/x DEO2
JMP2r

@draw-center-stats
    .Screen/width DEI2 #01 SFT2 #0008 SUB2 .Screen/x DEO2
    .Screen/height DEI2 #01 SFT2 #0004 SUB2 .Screen/y DEO2
    .current-val LDZ

    DUP #0a DIV         ( Get Tens )
    SWP #0a DIVk MUL SUB ( Get Ones )

    SWP #30 ADD ;draw-center-char JSR2
    #30 ADD ;draw-center-char JSR2
JMP2r

@draw-center-char ( char -- )
    #20 SUB            ( Align: Must match draw-char! )
    #00 SWP            ( Byte to Short )
    #30 SFT2           ( Multiply by 8 )
    ;font ADD2 .Screen/addr DEO2
    #41 .Screen/sprite DEO ( Color mode for center stats )
    .Screen/x DEI2 #0008 ADD2 .Screen/x DEO2
JMP2r

@clear-screen
    #00 .Screen/auto DEO #0000 .Screen/y DEO2
    @y #0000 .Screen/x DEO2
        @x #40 .Screen/pixel DEO #00 .Screen/pixel DEO
            .Screen/x DEI2 INC2 DUP2 .Screen/x DEO2
            .Screen/width DEI2 LTH2 ,x JCN
        POP2 .Screen/y DEI2 INC2 DUP2 .Screen/y DEO2
        .Screen/height DEI2 LTH2 ,y JCN
JMP2r

@draw-circle ( x* y* r* -- )
    .circle-r STZ2 .circle-cy STZ2 .circle-cx STZ2
    .circle-r LDZ2 DUP2 MUL2 .circle-rsq STZ2
    .circle-cy LDZ2 .circle-r LDZ2 SUB2
    @y-loop STH2 .circle-cx LDZ2 .circle-r LDZ2 SUB2
        @x-loop DUP2 .circle-cx LDZ2 SUB2 DUP2 MUL2
            STH2r DUP2 STH2 .circle-cy LDZ2 SUB2 DUP2 MUL2 ADD2
            .circle-rsq LDZ2 GTH2 ,skip-pixel JCN
                DUP2 .Screen/x DEO2 STH2r DUP2 STH2 .Screen/y DEO2 #03 .Screen/pixel DEO
            @skip-pixel #0001 ADD2
            DUP2 .circle-cx LDZ2 .circle-r LDZ2 ADD2 LTH2 ,x-loop JCN
        POP2 STH2r #0001 ADD2
        DUP2 .circle-cy LDZ2 .circle-r LDZ2 ADD2 LTH2 ,y-loop JCN
    POP2 JMP2r

@draw-zstr ( addr* -- )
    &loop
        LDAk DUP #00 EQU ,&end JCN ( If char is 0, we are done )
        ;draw-char JSR2             ( Draw the character )
        INC2 ,&loop JMP            ( Move to next character )
    &end
    POP2                           ( Remove the string address from stack )
JMP2r


( Check if mouse Y is within a row: y-offset -- flag )
@row-hit?
    SIDEBAR-Y ADD2
    DUP2 .Mouse/y DEI2 SWP2 LTH2 ,&no JCN
    BTN-SIZE ADD2
    .Mouse/y DEI2 SWP2 GTH2 ,&no JCN
    #01 JMP2r
    &no #00 JMP2r

( Check if mouse X is within a button: x-offset -- flag )
@btn-hit?
    SIDEBAR-X ADD2
    DUP2 .Mouse/x DEI2 SWP2 LTH2 ,&no JCN
    BTN-SIZE ADD2
    .Mouse/x DEI2 SWP2 GTH2 ,&no JCN
    #01 JMP2r
    &no #00 JMP2r


@load-rom ( filename* -- )
	.File/name DEO2
	( clear wst/rst )
	#0000 .System/wst DEO2
	( clear screen )
	#0000 DUP2 .Screen/x DEO2 .Screen/y DEO2
	[ LIT2 80 -Screen/pixel ] DEO
	[ LIT2 c0 -Screen/pixel ] DEO
	( reset device vectors )
	[ LIT2r 0000 ]
	#00
	&device-vector-loop
		DUP2r STHk DEO2r
		#10 ADD
		DUP ?&device-vector-loop
	POP POP2r
	( copy the zero-page-loader into f0-ff )
	;&zero-page-loader LITr f0
	&copy-loop
	LDAk STHkr STZ
	INC2 INCr
	STHkr ,&copy-loop JCN
	POP2 ( leave 00 on return stack )

	( prepare the stack for the zero-page-loader )
	( the more we prepare here in advance, the less we'll have to overwrite )
	STHkr #00fe ( arguments for STZ2 at ff )
	STHkr ( argument for JMP at fe  )
	DUPk #fcfe ( arguments for STZ2 at fd and JMP  )
	OVR2 #fafe ( arguments for STZ2 at fd and JMP  )
	OVR2 #f8fe ( arguments for STZ2 at fd and JMP  )
	OVR2 #f6fe ( arguments for STZ2 at fd and JMP  )
	OVR2 #f401 ( arguments for STZ2 at fd, plus an extra 01 )
	STHkr ( first argument for ADD2 )
	.File/success ( argument for DEI2 )
	#0100 .File/read ( arguments for DEO2 )
	#ff00 .File/length DEO2
	#00f0 JMP2

	&zero-page-loader
	( f0 ) DEO2
	( f1 ) DEI2
	( f2 ) ADD2
	( f3 ) &loop DUPr
	( f4 ) STH2k
	( f5 ) STAr
	( f6 ) INC2
	( f7 ) NEQ2k
	( f8 ) ,&loop
	( f9 )
	( fa ) JCN
	( fb ) POPr
	( fc ) POP2
	( fd ) STZ2 ( deletes f4-fd through looping )
	( fe ) JMP
	( ff ) STZ2 ( deletes fe-ff )

	&tmp $1


