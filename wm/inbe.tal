	#0000 .Screen/y DEO2
	#0000 .Screen/x DEO2
    @tbx #03 .Screen/sprite DEO
        .Screen/x DEI2 INC2 DUP2 .Screen/x DEO2
        .Screen/width DEI2 LTH2 ,tbx JCN
	JMP2r

