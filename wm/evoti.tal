@load-title-view ( -- )
    ( load menu )

	;load-menu JSR2

	RTN

( menu )
@load-menu ( -- )
    #01 .button-selected STZ
    #00 .button-pressed STZ

    ( init values for button generation )
    #01 .color STZ
    #0008 .margin STZ2
    #0075 .width STZ2
    #0045 .height STZ2
    #00d0 .y STZ2

    ( initialize x position )
    .margin LDZ2 .x STZ2

    ( loop through buttons )

    #00 .i STZ

    &loop
        .x LDZ2 .y LDZ2 .width LDZ2 .height LDZ2
        .i LDZ INC ;get-button-address JSR2

        ;set-size JSR2

        .x LDZ2 .width LDZ2 ADD2 .margin LDZ2 ADD2 .x STZ2

        .i LDZ INC .i STZ
        .i LDZ #04 LTH ,&loop JCN

	RTN


@on-frame-menu ( -- )
	;draw-title JSR2

    ( not moving 0 )
    #00 .Mouse/state DEI EQU .mouse-pressed LDZ ADD #02 EQU ,&mouse-release JCN
	,&skip1 JMP
	&mouse-release
        #00 .mouse-pressed STZ
        #00 .button-pressed LDZ NEQ ,&change JCN

    &skip1
	#00 .Controller/button DEI EQU .key-pressed LDZ ADD #02 EQU ,&key-release JCN
    ,&skip2 JMP

    &key-release
        #00 .key-pressed STZ
        #00 .button-pressed LDZ NEQ ,&change JCN
        ,&skip2 JMP

    &change
        .button-pressed LDZ .view STZ
    &skip2
    ;draw-buttons JSR2
	RTN

@draw-title ( -- )
	( logo )
	#01 .color STZ
	;logo .File/name DEO2
    .Screen/width DEI2 HALF2 LOGO-MARGIN SUB .Screen/x DEO2
    LOGO-MARGIN .Screen/y DEO2
	;load-img JSR2
    LOGO-MARGIN .Screen/y DEI2 ADD2 .Screen/y DEO2

	( title )
    .Screen/x DEI2 TITLE-X-OFFSET SUB2 .Screen/x DEO2
	;title-strings/title #02 ;draw-str JSR2

	( description )
	#00b5 .Screen/x DEO2 FONT-HEIGHT .Screen/y DEI2 ADD2 .Screen/y DEO2
	;title-strings/description1 #02 ;draw-str JSR2

    #00a7 .Screen/x DEO2 FONT-HEIGHT .Screen/y DEI2 ADD2 .Screen/y DEO2
	;title-strings/description2 #02 ;draw-str JSR2
	RTN

@draw-buttons ( -- )
    #0030 .offset-y STZ2
  	#01 .i STZ

    &loop
        .i LDZ DUP ;check-selected-button JSR2 ;draw-button JSR2

        .i LDZ INC .i STZ
        .i LDZ #05 LTH ,&loop JCN

	RTN

@on-mouse-menu ( -- )
	#01 DUP .buttons/1 ;check-hover-button JSR2 ;draw-button JSR2
	#02 DUP .buttons/2 ;check-hover-button JSR2 ;draw-button JSR2
	#03 DUP .buttons/3 ;check-hover-button JSR2 ;draw-button JSR2
	#04 DUP .buttons/4 ;check-hover-button JSR2 ;draw-button JSR2
	RTN

@on-controller-menu ( -- )

	( scroll through buttons using ctrl and alt )
	#02 .Controller/button DEI EQU ,&inc JCN
	,&skip1 JMP
	&inc
		.button-selected LDZ INC .button-selected STZ
	&skip1

	#01 .Controller/button DEI EQU ,&dec JCN
	,&skip2 JMP
	&dec
		.button-selected LDZ #01 SUB .button-selected STZ
	&skip2

	( carousel select )
	.button-selected LDZ #01 LTH ,&max JCN
	.button-selected LDZ #04 GTH ,&zero JCN
	,&skip3 JMP
	&zero
		#01 .button-selected STZ
		,&skip3 JMP
	&max
		#04 .button-selected STZ
	&skip3

	( check button pressed option )
	#04 .Controller/button DEI EQU ,&pressed JCN
	RTN
	&pressed
        #01 .key-pressed STZ
        .button-selected LDZ .button-pressed STZ
	RTN


@draw-button ( index -- )
    DUP #01 EQU ,&quick JCN
    DUP #02 EQU ,&group JCN
    DUP #03 EQU ,&book JCN
    #04 EQU ,&gear JCN
    RTN

    &quick
        ;menu-icons/clock #0028 #0000 ;menu-strings/quick
        .color LDZ #0006 .offset-y LDZ2 .buttons/1
        ,&draw JMP
    &group
        ;menu-icons/group #002c #0009 ;menu-strings/group
        .color LDZ #0025 .offset-y LDZ2 .buttons/2
        ,&draw JMP
    &book
        ;menu-icons/book #0030 #0009 ;menu-strings/book
        .color LDZ #0025 .offset-y LDZ2 .buttons/3
        ,&draw JMP
    &gear
        ;menu-icons/gear #002a #0005 ;menu-strings/gear
        .color LDZ #001a .offset-y LDZ2 .buttons/4
    &draw
    ;draw-btn JSR2
    RTN

