( Evoti )

( devices )
|00 @System     [ &vector $2 &pad     $6 &r      $2 &g     $2 &b     $2 ]
|10 @Console    [ &vector $2 &read    $1 &pad    $5 &write $1 ]
|20 @Screen     [ &vector $2 &width   $2 &height $2 &auto $1 &pad   $1 &x $2 &y $2 &addr $2 &pixel $1 &sprite $1 ]
|80 @Controller [ &vector $2 &button  $1 &key    $1 ]
|90 @Mouse      [ &vector $2 &x       $2 &y      $2 &state $1 &chord $1 ]
|a0 @File       [ &vector $2 &success $2 &offset $2 &pad   $2 &name  $2 &length $2 &load $2 &save $2 ]


( macros )
%HALF2 { #01 SFT2 } ( short -- short/2 )
%MOD { DIVk MUL SUB }
%MOD2 { DIV2k MUL2 SUB2 }
%RTN { JMP2r }
%EMIT { .Console/write DEO } ( character -- )
%NL { #0a EMIT } ( -- )


( zero page )
|00
@pointer [ &x $2 &y $2 &sprite $2 ]
@view $1
@pview $1
@screen-width $2
@screen-height $2
@mouse-pressed $1
@key-pressed $1
@buffer $80

( temp vars )
@color $1
@margin $2
@offset-y $2
@height $2
@width $2
@y $2
@x $2
@i $2

( button )
@button-selected $1
@button-pressed $1


( buttons )

@buttons
 &1 [ $2 $2 $2 $2 ]
 &2 [ $2 $2 $2 $2 ]
 &3 [ $2 $2 $2 $2 ]
 &4 [ $2 $2 $2 $2 ]

( init )
|100 @on-reset ( -> )
    ( set theme )
	;set-theme JSR2

    ( vectors )
	;on-controller .Controller/vector DEO2
    ;on-frame .Screen/vector DEO2
    ;on-mouse .Mouse/vector DEO2

	#0200 .screen-width STZ2
	#0140 .screen-height STZ2
	.screen-width LDZ2 .Screen/width DEO2
	.screen-height LDZ2 .Screen/height DEO2

	( set view )
	#00 .view STZ
	.view LDZ .pview STZ

	( set file length for img loading )
	#1000 .File/length DEO2

	;load-view JSR2


	.Screen/width DEI2 #01 SFT2 .Screen/x DEO2
	.Screen/height DEI2 #01 SFT2 .Screen/y DEO2
	#01 .Screen/pixel DEO
	#43 .Screen/pixel DEO
	#93 .Screen/pixel DEO
	#e2 .Screen/pixel DEO
	BRK


BRK

@load-view
	( load current view )
	.view LDZ #00 EQU ,&title JCN
	,&skip1 JMP
	&title
	;load-title-view JSR2
	&skip1

	.view LDZ #01 EQU ,&quick JCN
	RTN

	&quick
	;load-quick-view JSR2
	RTN

( theme )

@theme-txt ".theme $1

@set-theme ( -- )
	;theme-txt .File/name DEO2
	#0006 .File/length DEO2
	#fffa .File/load DEO2
	.File/success DEI2 #0006 NEQ2 ,&ignore JCN
		#fffa LDA2 .System/r DEO2
		#fffc LDA2 .System/g DEO2
		#fffe LDA2 .System/b DEO2
		&ignore

	RTN

( vectors )

@on-frame ( -> )
    ;clear-screen JSR2
    ;draw-pointer JSR2

	.pview LDZ .view LDZ NEQ ,&change-view JCN
	,&skip1 JMP

	&change-view
	.screen-width LDZ2 .Screen/width DEO2
	.screen-height LDZ2 .Screen/height DEO2

	#00 .Screen/addr DEO
	#00 .Screen/x DEO
	#00 .Screen/y DEO
	#00 .Screen/sprite DEO
	.view LDZ .pview STZ
	;load-view JSR2
	&skip1

	( current view on-frame )
	.view LDZ #00 EQU ,&title JCN
	,&skip2 JMP
	&title
	;on-frame-menu JSR2
	&skip2

	.view LDZ #01 EQU ,&quick JCN
	BRK
	&quick
	;on-frame-quick JSR2

	BRK


@clear-screen ( -- )
	.Screen/width DEI2 #01 SFT2 .Screen/x DEO2
	.Screen/height DEI2 #01 SFT2 .Screen/y DEO2

	( clear background )
	#80 .Screen/pixel DEO
	#90 .Screen/pixel DEO
	#a0 .Screen/pixel DEO
	#b0 .Screen/pixel DEO

	( clear foreground )
	#c0 .Screen/pixel DEO
	#d0 .Screen/pixel DEO
	#e0 .Screen/pixel DEO
	#f0 .Screen/pixel DEO
	RTN

@on-mouse ( -> )
	;pointer_icn .pointer/sprite STZ2

	( current view on-mouse )
	.view LDZ #00 EQU ,&title JCN
	,&skip1 JMP
	&title
	;on-mouse-menu JSR2
	&skip1

	.view LDZ #01 EQU ,&quick JCN
	,&skip2 JMP
	&quick
	;on-mouse-quick JSR2
	&skip2

    ;draw-pointer JSR2

	BRK

@on-controller ( -> )
	( current view on-controller )
	.view LDZ #00 EQU ,&title JCN
	,&skip1 JMP
	&title
	;on-controller-menu JSR2
	&skip1

	.view LDZ #01 EQU ,&quick JCN
	,&skip2 JMP
	&quick
	;on-controller-quick JSR2
	&skip2

	( exit or return to title screen )

	#1b .Controller/key DEI EQU ,&home JCN
	BRK
	&home
	#00 .view STZ


	BRK

( draw pointer )

@draw-pointer ( -- )

    ( set sprite address )
    .pointer/sprite LDZ2 .Screen/addr DEO2

    ( send pointer position to screen )
    .pointer/x LDZ2 .Screen/x DEO2
    .pointer/y LDZ2 .Screen/y DEO2

    #00 .Screen/auto DEO

    ( clear sprite from foreground )
	#40 .Screen/sprite DEO

	( update pointer positions )
	.Mouse/x DEI2 DUP2 .pointer/x STZ2 .Screen/x DEO2
	.Mouse/y DEI2 DUP2 .pointer/y STZ2 .Screen/y DEO2

	( draw new cursor )
	.Mouse/state DEI #01 EQU ,&pressed JCN
	#43 .Screen/sprite DEO
	RTN
	&pressed
		#41 .Screen/sprite DEO

RTN

@pointer_icn [ 80c0 e0f0 f8e0 1000 ]
@hand_icn    [ 4040 4070 f8f8 f870 ]

( utils )

@opposite-color ( color -- color )
	#01 #01 .color LDZ EQU ADD
	RTN

@print-button ( button index -- )
    #01 SUB #08 MUL .i STZ
    STHk .i LDZ ADD LDZ2 ;print-short JSR2
    .i LDZ #02 ADD .i STZ
    STHkr .i LDZ ADD LDZ2 ;print-short JSR2
    .i LDZ #02 ADD .i STZ
    STHkr .i LDZ ADD LDZ2 ;print-short JSR2
    .i LDZ #02 ADD .i STZ
    STHr .i LDZ ADD LDZ2 ;print-short JSR2
    #0a EMIT
    RTN

@print-4-shorts ( short short short short -- )
    STH2 ROT2 ;print-short JSR2
    SWP2 ;print-short JSR2
    ;print-short JSR2
    STH2r ;print-short JSR2
    #0a EMIT
    RTN

@print-short ( short -- )
    SWP #ff AND ;print-byte JSR2
    #ff AND ;print-byte JSR2
    RTN

@print-byte ( byte -- )
    DUP #04 SFT #0f AND ;hex-to-ascii JSR2 EMIT
    #0f AND ;hex-to-ascii JSR2 EMIT
    RTN

@hex-to-ascii ( nibble -- ascii )
    DUP #0a LTH ,&digit JCN
    #57 ADD RTN
    &digit #30 ADD RTN


@slen ( str* -- len* )
	DUP2 ,scap JSR SWP2 SUB2
	RTN

@scap ( str* -- str-end* )
	( clamp ) LDAk #00 NEQ JMP RTN
	&while INC2 LDAk ,&while JCN
	RTN

@sput ( str* char -- )
	ROT ROT ,scap JSR STA
	RTN

@spop ( str* -- )
	( clamp ) LDAk #00 NEQ JMP RTN
	#00 ROT ROT ,scap JSR #0001 SUB2 STA
	RTN

@mclr ( addr* len* -- )
	OVR2 ADD2 SWP2
	&loop
		STH2k #00 STH2r STA
		INC2 GTH2k ,&loop JCN
	POP2 POP2
	RTN


( ui )

@get-button-address ( index -- button-address )
    DUP #01 EQU ,&btn1 JCN
    DUP #02 EQU ,&btn2 JCN
    DUP #03 EQU ,&btn3 JCN
    DUP #04 EQU ,&btn4 JCN
    ( Default case: return null or handle error )
    #0000 RTN

    &btn1 POP ;buttons/1 RTN
    &btn2 POP ;buttons/2 RTN
    &btn3 POP ;buttons/3 RTN
    &btn4 POP ;buttons/4 RTN


@set-size ( x1 y1 w* h* rect* -- )
	STH2
	( size to rect )
	STH2 STH2 OVR2 STH2r ADD2 OVR2 STH2r ADD2
	STH2r
	DUP2 ROT2 SWP2 #0006 ADD2 STA2
	DUP2 ROT2 SWP2 #0004 ADD2 STA2
	DUP2 ROT2 SWP2 INC2 INC2 STA2
	DUP2 ROT2 SWP2 STA2
	POP2
	RTN

@within-rect ( x* y* rect -- flag )
	STH
	( y LTH rect.y ) DUP2 STHkr INC INC LDZ2 LTH2 ,&skip JCN
	( y GTH rect.w ) DUP2 STHkr #06 ADD LDZ2 GTH2 ,&skip JCN
	SWP2
	( x LTH rect.x ) DUP2 STHkr LDZ2 LTH2 ,&skip JCN
	( x GTH rect.h ) DUP2 STHkr #04 ADD LDZ2 GTH2 ,&skip JCN
	POP2 POP2 POPr
	#01
	RTN
	&skip
		POP2 POP2 POPr
		#00
		RTN


@check-hover-button ( selected rect -- )
	STH
	.Mouse/x DEI2 .Mouse/y DEI2 STHr ;within-rect JSR2 #00 EQU ,&skip JCN
		;hand_icn .pointer/sprite STZ2
		DUP .button-selected STZ

		.Mouse/state DEI #01 EQU ,&pressed JCN
		POP
		RTN
	&pressed
        #01 .mouse-pressed STZ
		DUP .button-pressed STZ
	&skip
	POP
	RTN

@check-selected-button ( key -- )
	#01 .color STZ

	DUP .button-pressed LDZ EQU ,&pressed JCN
	DUP .button-selected LDZ EQU ,&selected JCN
	,&rtn JMP
	&selected
		#01 .key-pressed LDZ EQU ,&pressed JCN
		#03 .color STZ
		,&rtn JMP
	&pressed
		#02 .color STZ
	&rtn
	POP
	RTN

@draw-btn ( image offset-x offset-y str* color offset-x offset-y rect -- )
    STH
    ( add offset to text and draw text )
    STHkr INC INC LDZ2 ADD2 .Screen/y DEO2
    STHkr LDZ2 ADD2 .Screen/x DEO2
    ;draw-str JSR2

    STHkr INC INC LDZ2 ADD2 .Screen/y DEO2
    STHr LDZ2 ADD2 .Screen/x DEO2

	.File/name DEO2 ;load-img JSR2
	RTN

@draw-text-btn ( str* color pos-x pos-y -- )
	.Screen/y DEO2
	.Screen/x DEO2
	;draw-str JSR2
	RTN

@draw-icon-btn ( image pos-x pos-y -- )
	.Screen/y DEO2
	.Screen/x DEO2
	.File/name DEO2 ;load-img JSR2
	RTN


@load-img ( color str* -- )
	;&img .File/load DEO2
      	;&img .Screen/addr DEO2
        #f6 .Screen/auto DEO
        #0400
        &loop
            .color LDZ #80 ADD .Screen/sprite DEO
			INC GTHk ,&loop JCN
        POP2
	RTN
	&img $1000


@draw-str ( str* color -- )
    #06 .Screen/auto DEO
	STH
	&while
		LDAk STHkr ,draw-char JSR
		INC2 LDAk ,&while JCN
	POP2
	POPr
	RTN

@draw-char ( char color -- )
	SWP
	[ #20 SUB #00 SWP #40 SFT2 ;font ADD2 ] .Screen/addr DEO2
	.Screen/sprite DEOk DEO
	.Screen/y DEI2 #0010 SUB2 .Screen/y DEO2
	.Screen/x DEI2 #0008 ADD2 .Screen/x DEO2
	RTN

